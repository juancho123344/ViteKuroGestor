{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      },
      {
        "use": "@swc/core",
        "input": "src",
        "output": "dist",
        "config": {
          "jsc": {
            "target": "es2018"
          }
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  